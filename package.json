{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --proxy-config proxy.dev.conf.js",
    "build": "ng build --prod",
    "lint": "ng lint && eslint -c .eslintrc.js --ext .ts \"src\" && prettier --list-different \"src/**/*.ts\"",
    "lint:fix": "ng lint --fix && eslint -c .eslintrc.js --fix --ext .ts \"src\" && prettier --write \"src/**/*.ts\"",
    "lint:eslint": "eslint -c .eslintrc.js --ext .ts \"src\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "e2e": "ng e2e",
    "jest": "jest",
    "jest:ci": "jest --ci --coverage --colors",
    "jest:changed": "jest --onlyChanged",
    "jest:changed:watch": "jest --onlyChanged --watch",
    "jest:watch": "jest --watch",
    "jest:clear": "jest --clearCache"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/common": "^9.1.11",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.1.11",
    "@angular/forms": "^9.1.11",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/router": "^9.1.11",
    "rxjs": "^6.5.5",
    "tslib": "^1.13.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.9",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
    "@angular/cli": "^9.1.9",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "^9.1.11",
    "@types/jest": "^26.0.3",
    "@types/node": "^13.13.12",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "codelyzer": "^5.2.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsdoc": "^28.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "prettier": "2.0.5",
    "protractor": "^5.4.4",
    "ts-node": "~8.8.2",
    "typescript": "~3.8.3"
  }
}
